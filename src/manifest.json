{
  "manifest_version": 3,
  "name": "PhotoManiac",
  "version": "1.0.0",
  "description": "Present spectacular photos every time you open a new tab.",
  "author": "Henry Li",
  "permissions": [
    "storage"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "background": {
    "service_worker": "background/index.ts"
  },
  "chrome_url_overrides": {
    "newtab": "page/index.html"
  },
  "icons": {
    "16": "icons/icon-16.png",
    "32": "icons/icon-32.png",
    "48": "icons/icon-48.png",
    "128": "icons/icon-128.png"
  }
}
